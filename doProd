# Compile js
jsmin <public/dev/js/main.js >public/dev/js/main.min.js

# Concatenate 3 needed js files
cat public/dev/js/libs/jquery.min.js public/dev/js/libs/jquery.jplayer.min.js public/dev/js/main.min.js > dist/debug/jvkoh_website.app.js

# Minify js and css
grunt release

cp -r dist/release public/
