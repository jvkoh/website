# Compile js
jsmin <assets/js/main.js >assets/js/main.min.js
jsmin <assets/js/life.js >assets/js/life.min.js

# Concatenate 3 needed js files
cat assets/js/libs/jquery.jplayer.min.js assets/js/main.min.js > public/js/javascript.min.js
scp assets/js/libs/jquery.min.js public/js/jquery.min.js
scp assets/js/life.min.js public/js/life.min.js

scp assets/css/master.css public/css/master.css
scp assets/css/resume.css public/css/resume.css
